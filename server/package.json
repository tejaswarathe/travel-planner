{
  "name": "travel-planner-app",
  "version": "1.0.0",
  "description": "A travel Planner App using MERN stack",
  "main": "server.ts",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "preserve": "npm run build",
    "serve": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\""
  },
  "author": "Tejas Warathe",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.1.0",
    "npx": "^10.2.2",
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.0",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  }
}
